@model IEnumerable<RegattaManager.ViewModels.Mannschaftswertung>

@{
    ViewData["Title"] = "Mannschaftswertung";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Mannschaftswertung</h2>

<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ClubName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.OldclassName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Wertung)
                </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ClubName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OldclassName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Wertung)
            </td>
        </tr>
}
    </tbody>
</table>
